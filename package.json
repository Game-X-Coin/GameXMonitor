{
  "name": "gamexmonitor",
  "version": "0.1.0",
  "private": true,
  "description": "Track the history of GameXCoin",
  "main": "index.js",
  "homepage": "https://monitor.gxc.world/",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/app.*",
    "precommit": "lint-staged",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\" --syntax scss",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix .",
    "fix": "yarn run fix-js && yarn run fix-css",
    "flow": "flow",
    "flow:check": "flow check",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-coverage": "jest --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "start": "webpack-dev-server --inline --hot --config config/webpack.config.dev.js",
    "build": "rimraf ./build && webpack --config config/webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Game-X-Coin/GameXMonitor"
  },
  "keywords": [
    "react",
    "reactjs",
    "mobx"
  ],
  "author": "GXC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Game-X-Coin/GameXMonitor/issues"
  },
  "flow": "flow",
  "lint-staged": {
    "*.{js,jsx,mjs}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint-staged",
  "devDependencies": {
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.0.0-alpha.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "css-hot-loader": "^1.3.9",
    "css-loader": "0.28.11",
    "dotenv-webpack": "^1.5.5",
    "eslint": "4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.70.0",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-localstorage-mock": "^2.2.0",
    "jest-transform-stub": "^1.0.0",
    "lint-staged": "^7.0.4",
    "node-sass": "^4.8.3",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.3",
    "pre-commit": "^1.2.2",
    "precss": "^3.1.2",
    "prettier": "^1.12.1",
    "react-dev-utils": "^5.0.0",
    "react-hot-loader": "^4.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "source-map-explorer": "^1.5.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "webpack": "^3",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^2",
    "webpack-hot-middleware": "^2.22.0",
    "webpack-manifest-plugin": "2.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.0",
    "classnames": "^2.2.5",
    "dayjs": "^1.6.10",
    "mobx": "^4.2.0",
    "mobx-react": "^5.1.2",
    "mobx-react-devtools": "^5.0.1",
    "mobx-react-router": "^4.0.2",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "query-string": "^6.1.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.5.3",
    "react-json-view": "^1.19.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^6.0.1",
    "whatwg-fetch": "^2.0.4"
  }
}
